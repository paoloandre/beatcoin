          Planned Payments
        </md-nav-item>
        <md-nav-item ng-if="!isAuthenticated()" role="send" md-nav-href="/#!/login" name="login">
          Login
        </md-nav-item>
        <md-nav-item ng-if="!isAuthenticated()" role="send" md-nav-href="/#!/signup" name="signup">
          Sign Up
        </md-nav-item>
        <md-nav-item ng-if="isAuthenticated()" role="send" md-nav-href="/#!/logout" name="logout">
          Logout
        </md-nav-item>
      </md-nav-bar>
    </md-content>
